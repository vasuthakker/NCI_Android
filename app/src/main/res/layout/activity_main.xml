<?xml version="1.0" encoding="utf-8"?>

<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.epuser.pickcontacts.activities.MainActivity"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="81dp">



    <View
        android:layout_width="0dp"
        android:layout_height="5dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:background="@color/colorPrimary"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@drawable/ic_accept"
        android:id="@+id/view"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintTop_toTopOf="@+id/main_imgadd"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.01999998" />

    <ImageView
        android:id="@+id/main_imgbalance"
        android:layout_width="135dp"
        android:layout_height="135dp"
        android:background="@drawable/bg_icon"
        android:padding="45dp"
        android:tint="@color/colorPrimary"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/ic_balance"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="50dp"
        app:layout_constraintRight_toLeftOf="@+id/main_imgadd"
        android:layout_marginRight="8dp"
        app:layout_constraintLeft_toRightOf="@+id/main_imgpay"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintVertical_bias="0.110000014" />

    <ImageView
        android:tint="@color/colorPrimary"
        android:id="@+id/main_imgaccept"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:foregroundGravity=""
        android:layout_marginEnd="8dp"
        android:scaleType="fitXY"
        android:padding="35dp"
        app:srcCompat="@drawable/ic_accept"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.068"
        app:layout_constraintLeft_toRightOf="@+id/main_imgbalance"
        android:layout_marginLeft="5dp"
        app:layout_constraintHorizontal_bias="1.0"
        android:layout_marginStart="5dp" />

    <ImageView
        android:id="@+id/main_imgpay"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:padding="35dp"
        android:scaleType="fitXY"
        android:tint="@color/colorPrimary"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_pay"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.077" />

    <ImageView
        android:tint="@color/colorPrimary"
        android:id="@+id/main_imgreqmoney"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:padding="35dp"
        app:srcCompat="@drawable/ic_reqmoney"
        android:layout_marginLeft="8dp"
        android:scaleType="fitXY"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="8dp"
        app:layout_constraintTop_toBottomOf="@+id/main_imgpay" />

    <ImageView
        android:tint="@color/colorPrimary"
        android:id="@+id/main_imgadd"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:padding="35dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/ic_addmoney"
        app:layout_constraintTop_toBottomOf="@+id/main_imgaccept"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginEnd="8dp" />

    <TextView
        android:id="@+id/editText"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="4dp"
        android:layout_marginLeft="15dp"
        android:layout_marginStart="15dp"
        android:gravity="center"
        android:scaleType="fitXY"
        android:text="@string/send"
        android:textColor="@color/colorPrimary"
        app:layout_constraintBottom_toBottomOf="@+id/main_imgpay"
        app:layout_constraintLeft_toLeftOf="@+id/main_imgpay" />

    <TextView
        android:id="@+id/editText2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="1dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:gravity="center"

        android:scaleType="fitXY"
        android:text="@string/receive_money"

        android:textColor="@color/colorPrimary"
        app:layout_constraintBottom_toTopOf="@+id/main_imgadd"
        app:layout_constraintRight_toRightOf="@+id/main_imgaccept" />

    <TextView
        android:id="@+id/editText3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="19dp"
        android:layout_marginLeft="45dp"
        android:scaleType="fitXY"
        android:text="@string/balance"
        android:gravity="center"
        android:textColor="@color/colorPrimary"
        app:layout_constraintBottom_toBottomOf="@+id/main_imgbalance"
        app:layout_constraintLeft_toLeftOf="@+id/main_imgbalance"
        android:layout_marginStart="45dp" />

    <TextView
        android:id="@+id/editText5"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="4dp"
        android:scaleType="fitXY"
        android:layout_marginEnd="15dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:inputType="textPersonName"
        android:text="@string/add_money"
        android:gravity="center"
        android:textColor="@color/colorPrimary"
        app:layout_constraintBottom_toBottomOf="@+id/main_imgadd"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintLeft_toLeftOf="@+id/main_imgadd"
        app:layout_constraintRight_toRightOf="@+id/main_imgadd"
        android:layout_marginStart="15dp" />


    <TextView
        android:id="@+id/editText4"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:layout_marginLeft="15dp"
        android:layout_marginStart="15dp"
        android:scaleType="fitXY"
        android:inputType="textPersonName"
        android:gravity="center"
        android:text="@string/req_money"
        android:textColor="@color/colorPrimary"
        app:layout_constraintBottom_toBottomOf="@+id/main_imgreqmoney"
        app:layout_constraintLeft_toLeftOf="@+id/main_imgreqmoney" />

    <FrameLayout
        android:id="@+id/fcontainer"
        android:layout_width="0dp"
        android:layout_height="wrap_content"

        android:layout_marginBottom="188dp"
        android:layout_weight="2.77"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintVertical_bias="0.85"
        android:layout_marginEnd="8dp" />



    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar5"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="?attr/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:title="HOME"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHorizontal_bias="0.0" />

    <RelativeLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp">

    </RelativeLayout>

</android.support.constraint.ConstraintLayout>






